<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<AnchorPane prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.LinkOverviewController">
   <children>
      <Label text="Link List" />
      <ListView fx:id="_LinkArray" AnchorPane.topAnchor="20.0"/>
      <Button fx:id="btnAddLink" text="Add Link" onAction="#handleBtnAddLinkAction" AnchorPane.rightAnchor="30"/>
      <Button fx:id="btnDelLink" text="Delete Link" onAction="#handleBtnDelLinkAction" AnchorPane.rightAnchor="30" AnchorPane.topAnchor="50"/>
      <Button fx:id="btnEditLink" text="Edit Link" onAction="#handleBtnEditLinkAction" AnchorPane.rightAnchor="30" AnchorPane.topAnchor="100"/>
   </children>
</AnchorPane>
